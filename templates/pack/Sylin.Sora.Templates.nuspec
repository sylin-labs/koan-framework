<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Sylin.Sora.Templates</id>
    <version>0.1.0-preview</version>
    <authors>Sylin Labs</authors>
    <owners>Sylin Labs</owners>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/sylin-labs/sora-framework</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Project templates for Sora: TinyApi (JSON), TinyApp (SQLite), TinyDockerApp (Alpine.js + Mongo + Compose), TinyWorker.</description>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
    <tags>dotnet-new sora sylin templates tiny api web docker worker</tags>
  </metadata>
  <files>
    <file src="..\TinyApi\**\*" target="content\TinyApi" />
    <file src="..\TinyApp\**\*" target="content\TinyApp" />
    <file src="..\TinyDockerApp\**\*" target="content\TinyDockerApp" />
    <file src="..\TinyWorker\**\*" target="content\TinyWorker" />
  </files>
</package>