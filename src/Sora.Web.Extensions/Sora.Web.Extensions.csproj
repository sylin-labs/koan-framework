<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Web capability extensions (moderation, soft-delete, audit) for Sora applications.</Description>
    <PackageTags>$(CommonPackageTags);web;aspnetcore;controllers;capabilities;moderation;soft-delete;audit</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Sora.Data.Core\Sora.Data.Core.csproj" />
    <ProjectReference Include="..\Sora.Data.Abstractions\Sora.Data.Abstractions.csproj" />
    <ProjectReference Include="..\Sora.Core\Sora.Core.csproj" />
    <ProjectReference Include="..\Sora.Web\Sora.Web.csproj" />
  </ItemGroup>
</Project>
