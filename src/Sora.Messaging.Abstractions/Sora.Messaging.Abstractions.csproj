<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <LangVersion>preview</LangVersion>
    <AssemblyName>Sora.Messaging.Abstractions</AssemblyName>
    <RootNamespace>Sora.Messaging</RootNamespace>
  <Description>Messaging abstractions for Sora: contracts for buses, envelopes, and inbox/outbox patterns.</Description>
  <PackageTags>$(CommonPackageTags);messaging;abstractions;contracts</PackageTags>
  </PropertyGroup>
</Project>
