<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  <Description>Data core for Sora: repository primitives, options, and shared helpers for relational and document providers.</Description>
  <PackageTags>$(CommonPackageTags);data;repository;abstractions;options</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Sora.Data.Abstractions\Sora.Data.Abstractions.csproj" />
  <ProjectReference Include="..\Sora.Data.Vector.Abstractions\Sora.Data.Vector.Abstractions.csproj" />
    <ProjectReference Include="..\Sora.Core\Sora.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.8" />
  <PackageReference Include="Microsoft.Extensions.Configuration" Version="9.0.0" />
  <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="9.0.0" />
  <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="9.0.0" />
  <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="9.0.0" />
  </ItemGroup>
</Project>
