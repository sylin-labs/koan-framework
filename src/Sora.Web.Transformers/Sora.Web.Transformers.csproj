<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  <Description>HTTP payload transformers for Sora Web: flexible request/response shaping with auto-discovery.</Description>
  <PackageTags>$(CommonPackageTags);web;transformers;middleware;http</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Sora.Web\Sora.Web.csproj" />
    <ProjectReference Include="..\Sora.Core\Sora.Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.0" />
  </ItemGroup>
</Project>
