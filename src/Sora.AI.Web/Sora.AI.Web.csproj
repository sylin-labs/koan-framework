<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Sora AI Web: optional AI controllers/endpoints (chat, embeddings, health) for ASP.NET Core without coupling Sora.Web to AI.</Description>
    <PackageTags>$(CommonPackageTags);web;aspnetcore;controllers;ai;chat;embeddings</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  <ItemGroup>
    <Using Include="Sora.Core" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Sora.AI\Sora.AI.csproj" />
    <ProjectReference Include="..\Sora.AI.Contracts\Sora.AI.Contracts.csproj" />
  </ItemGroup>
</Project>
