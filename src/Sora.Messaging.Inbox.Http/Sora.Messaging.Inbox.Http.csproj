<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>Sora.Messaging.Inbox.Http</AssemblyName>
    <RootNamespace>Sora.Messaging.Inbox.Http</RootNamespace>
  <Description>HTTP-based inbox store for Sora Messaging: simple and configurable receiver endpoint.</Description>
  <PackageTags>$(CommonPackageTags);messaging;inbox;http</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Sora.Messaging.Abstractions\Sora.Messaging.Abstractions.csproj" />
    <ProjectReference Include="..\Sora.Messaging.Core\Sora.Messaging.Core.csproj" />
    <ProjectReference Include="..\Sora.Core\Sora.Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.8" />
  </ItemGroup>
</Project>
