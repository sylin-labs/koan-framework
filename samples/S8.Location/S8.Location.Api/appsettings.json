{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "S8.Location": "Debug"
    }
  },
  "AllowedHosts": "*",
  "S8": {
    "Location": {
      "Orchestrator": {
        "ProcessingMode": "Sequential",
        "TimeoutSeconds": 30,
        "MaxRetries": 3
      },
      "Resolution": {
        "CacheEnabled": true,
        "CacheTTLHours": 720,
        "NormalizationRules": {
          "CaseMode": "Upper",
          "RemovePunctuation": true,
          "CompressWhitespace": true
        }
      },
      "Geocoding": {
        "Primary": "GoogleMaps",
        "Fallback": "OpenStreetMap",
        "MaxMonthlyBudget": 250.00
      },
      "Ai": {
        "Model": "llama3.1:8b",
        "TimeoutMs": 5000
      }
    }
  }
}