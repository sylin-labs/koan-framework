{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Sora": {
    "Data": {
      "Postgres": {
        "ConnectionString": "Host=localhost;Port=5432;Database=sora_s7;Username=postgres;Password=postgres",
        "DdlPolicy": "AutoCreate",
        "SchemaMatchingMode": "Relaxed",
        "AllowProductionDdl": false,
        "DefaultPageSize": 50,
        "MaxPageSize": 200,
        "SearchPath": "public"
      }
    },
    "Web": {
      "Auth": {
        "ReturnUrl": {
          "DefaultPath": "/"
        },
        "Providers": {}
      }
    }
  },
  "Sora:Web:Auth:TestProvider": {
    "Enabled": true,
    "ClientId": "test-client",
    "ClientSecret": "test-secret",
    "ExposeInDiscoveryOutsideDevelopment": false
  }
}
